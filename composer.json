{
    "name": "floatingpoint/stylist",
    "description": "Laravel 5 theming package.",
    "authors": [
        {
            "name": "Kirk Bushell",
            "email": "torm3nt@gmail.com"
        },
        {
            "name": "Mike Dugan",
            "email": "mike@mjdugan.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/html": "5.0.*",
        "illuminate/support": "5.0.*"
    },
    "require-dev": {
        "mockery/mockery": "dev-master@dev",
        "orchestra/testbench": "3.0.*@dev",
        "phpunit/phpunit": "4.2.*",
        "barryvdh/laravel-ide-helper": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "FloatingPoint\\Stylist\\": "src/Stylist",
            "Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "vcs",
            "url": "git://github.com/orchestral/phpseclib.git"
        }
    ],
	"config": {
		"preferred-install": "dist"
	}
}
